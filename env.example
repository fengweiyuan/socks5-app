# SOCKS5代理服务器环境变量配置示例
# 复制此文件为 .env 并修改相应的值

# 服务器配置
SERVER_PORT=8012
SERVER_HOST=0.0.0.0
SERVER_MODE=debug
JWT_SECRET=your-secret-key-change-this-in-production

# 数据库配置
DB_DRIVER=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=socks5_user
DB_PASSWORD=socks5_password
DB_DATABASE=socks5_db

# 代理配置
PROXY_PORT=1080
PROXY_HOST=0.0.0.0
PROXY_TIMEOUT=30
PROXY_MAX_CONNECTIONS=1000

# 认证配置
AUTH_SESSION_TIMEOUT=3600
AUTH_MAX_LOGIN_ATTEMPTS=5

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log
LOG_FORMAT=json

# 前端配置
VITE_API_URL=http://localhost:8012/api/v1
